{
    "name": "l91/styleguide-bundle",
    "description": "A bundle to create styleguide pages.",
    "type": "sulu-bundle",
    "require": {
        "symfony/config": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/framework-bundle": "^3.4",
        "symfony/http-foundation": "^3.4",
        "symfony/http-kernel": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "L91\\Bundle\\StyleguideBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "keywords": [
        "styleguide",
        "symfony-bundle",
        "sulu-bundle"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Schranz",
            "email": "alexander@sulu.io"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
